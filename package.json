{
  "name": "vim2html",
  "version": "0.0.4",
  "description": "Create syntax-highlighted HTML from source code using Vim's 2html script",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "clean": "rm -r apidocs lib ; true",
    "prepublish": "npm run compile;bash setup-vim-rtp.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kba/node-vim2html"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kba/node-vim2html/issues"
  },
  "homepage": "https://github.com/kba/node-vim2html",
  "dependencies": {
    "merge": "^1.2.0",
    "mv": "^2.0.3",
    "temp": "^0.8.1"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "coffee-script": "^1.9.1",
    "docker": "^0.2.14",
    "tap-spec": "^2.2.2",
    "tapes": "^0.4.1"
  },
  "main": "lib/index.js"
}
